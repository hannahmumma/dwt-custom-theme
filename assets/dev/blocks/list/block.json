{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "dwt/list",
	"version": "0.1.0",
	"title": "list",
	"category": "prh-modules",
	"icon": "",
	"description": "A block to render a book or contributor list",
	"supports": {
		"html": false,
        "lock": false
	},
    "example": {},
    "parent": [],
    "attributes": {
        "islist": {
            "default": true,
            "type": "boolean"
        },
        "ctaSlideText": {
            "default": [],
            "type": "array"
        },
        "ctaLinkUrl": {
        	"default": "",
            "type": "string"
        },
        "ctaLinkTarget": {
        	"default": false,
            "type": "boolean"
        }, 
        "allowedMetadataFields": {
            "default": [],
            "type": "array"
        },
        "allowedContributorRoles": {
            "default": [],
            "type": "array"
        },
        "innerBlocksAttrs": {
            "default": [],
            "type": "array"
        },       
        "parentAttrs": {
            "default": {},
            "type": "object"
        }
	},
	"textdomain": "dwt-custom-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
